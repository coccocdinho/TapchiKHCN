<%@ Page Title="" Language="C#" MasterPageFile="~/Blank.master" AutoEventWireup="true" CodeFile="JournalAddPage.aspx.cs" Inherits="JournalAddPage" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="Server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="Server">
    <div class="form-group">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Tên Chuyên san</label>
        <div class="col-sm-9">
            <asp:TextBox runat="server" ID="txtName" placeholder="Tên Chuyên san" class="form-control" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Mã Chuyên san</label>
        <div class="col-sm-9">
            <asp:TextBox runat="server" ID="txtCode" placeholder="Tên Chuyên san" class="form-control" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Tài khoản Quản trị</label>
        <div class="col-sm-9">
            <asp:DropDownList runat="server" ID="ddlUser" Checked="true" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Tài khoản Thư ký</label>
        <div class="col-sm-9">
            <asp:DropDownList runat="server" ID="ddlThuKy" Checked="true" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Sử dụng</label>
        <div class="col-sm-9">
            <asp:CheckBox runat="server" ID="chkIsActive" Checked="true" />
        </div>
    </div>
    <div class="col-xs-12">
        <div class="col-xs-5"></div>
        <div class="col-xs-6">
            <asp:Button runat="server" ID="btnSave" CssClass="btn btn-info btn-sm" Text="Ghi lại" OnClick="btnSave_Click" />
            <asp:Label runat="server" ID="lblMessage"></asp:Label>
        </div>
    </div>
</asp:Content>

